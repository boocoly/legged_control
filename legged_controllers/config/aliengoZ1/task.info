torqueLimitsTask
{
   (0,0)  35.278   ; HAA
   (1,0)  35.278   ; HFE
   (2,0)  44.4     ; KFE
   (3,0)  30       ; ARM
}

frictionConeTask
{
  frictionCoefficient    0.3
}

weight
{
    endEffector     100
    swingLeg        100
    baseAccel       1
