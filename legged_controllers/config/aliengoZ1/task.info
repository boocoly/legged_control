endEffector
{
; end effector quadratic penalty scaling
  muPosition      2000.0 ;
  muOrientation    1000.0 ;
}

finalEndEffector
{
  muPosition      2000.0 ;
  muOrientation    1000.0 ;
}

frictionConeSoftConstraint
{
  frictionCoefficient    0.3

  ; relaxed log barrier parameters
  mu                     0.1
  delta                  5.0
}


jointPositionLimits
{
  ; relaxed log barrier mu
  mu      0.1

  ; relaxed log barrier delta
  delta   1e-3
}

jointVelocityLimits
{
  ; relaxed log barrier mu
  mu      0.5   ; 0.1

  ; relaxed log barrier delta
  delta   1e-3

  lowerBound
  {
    ; velocity limits for the arm DOFs
    arm
    {
      (0,0)  -1.2    ; arm_1 [rad/s]
      (1,0)  -1.2    ; arm_2 [rad/s]
      (2,0)  -1.2    ; arm_3 [rad/s]
